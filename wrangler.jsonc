{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ai-email-agent",
  "main": "src/server/index.ts",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "assets": {
    "directory": "public",
    "not_found_handling": "single-page-application",
    "run_worker_first": [
      "/api/*"
    ]
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Agent",
        "class_name": "Agent"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v2",
      "new_sqlite_classes": ["Agent"]
    }
  ] ,
  "d1_databases": [{
    "binding": "DB",
    "database_name": "cf_ai_email_agent",
    "database_id": "9780cf57-dc85-4aca-bbb5-916fc84e0881",
    "migrations_dir": "./src/server/db/migrations"
  }]
}


